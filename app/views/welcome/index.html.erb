<h2>The Business Model Canvas</h2>

<div class="canvas panel panel-info">
<div class="panel-heading">
    <h3 class="panel-title">Key Partners</h3>
  </div>
  <div class="panel-body">
	<div class="input">
		<%= form_tag("/add", method: "post") do %>
  		<%= text_field_tag(:title) %>
		<% end %>
	</div>
	<div class="items">
		<ul>
		<% @items.each do |item| %>
		<li><span class="label label-primary"><%=  item.title %></span></li>
		<% end %>
		</ul>
	</div>

	<div class="footer">
		<%= form_tag("/clear", method: "post") do %>
		<%= submit_tag("Clear Items", class: 'btn btn-default navbar-btn') %>
		<% end %>
	</div>
	 </div>
</div>